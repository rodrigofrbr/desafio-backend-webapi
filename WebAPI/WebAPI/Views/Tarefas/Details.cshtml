@model WebAPI.Models.Tarefa

@{
    ViewData["Title"] = "Details";
}

<div>
    <dl class="row mt-4">
        <dt class = "col-sm-2">
            Título
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Titulo)
        </dd>
        <dt class = "col-sm-2">
            Descrição
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
        <dt class = "col-sm-2">
            Criado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataCriar)
        </dd>
        <dt class = "col-sm-2">
            Última Atualização
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataUpdate)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-outline-dark" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
    <a class="btn btn-outline-dark" asp-action="Index">Voltar</a>
</div>
